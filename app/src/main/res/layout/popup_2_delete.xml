<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.cardview.widget.CardView
        android:id="@+id/CardViewPopUpDeleteDua"
        android:layout_width="337dp"
        android:layout_height="269dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:cardCornerRadius="40dp">

        <ImageView
            android:id="@+id/succeed"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/succeed"
            android:layout_marginLeft="118dp"
            android:layout_marginTop="20dp"></ImageView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="SUCCEED!"
            android:fontFamily="@font/poppins_bold"
            android:textSize="18sp"
            android:textColor="@color/black"
            android:layout_marginTop="142dp"
            android:layout_marginLeft="125dp"></TextView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Your data has been deleted successfully."
            android:fontFamily="@font/poppins_medium"
            android:textSize="12sp"
            android:layout_marginTop="165dp"
            android:layout_marginLeft="45dp"></TextView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="To check click the button below"
            android:fontFamily="@font/poppins_medium"
            android:textSize="12sp"
            android:layout_marginTop="185dp"
            android:layout_marginLeft="65dp"></TextView>

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/backHomeDeleted"
            android:layout_width="150dp"
            android:layout_height="48dp"
            android:layout_marginLeft="100dp"
            android:layout_marginTop="225dp"
            android:layout_marginBottom="100dp"
            android:background="@android:color/transparent"
            android:fontFamily="@font/poppins_medium"
            android:text="Home"></androidx.appcompat.widget.AppCompatButton>







    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>